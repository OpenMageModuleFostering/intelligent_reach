             xmlns="http://schemas.xmlsoap.org/wsdl/"
             name="{{var wsdl.name}}" targetNamespace="urn:{{var wsdl.name}}">
    <message name="shoppingCartShippingCustomIRMethodRequest">
        <part name="sessionId" type="xsd:string"/>
