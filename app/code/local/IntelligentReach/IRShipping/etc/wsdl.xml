				<part name="price" type="xsd:double"/>
				<part name="description" type="xsd:string"/>
        <part name="storeId" type="xsd:string"/>
    </message>
    <message name="shoppingCartShippingCustomIRMethodResponse">
        <part name="result" type="xsd:boolean"/>
